<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="D4w_cross.Views.MDPage"
             xmlns:pages="clr-namespace:D4w_cross.Views"
             Title="FIND"
    x:Name="MDPage">
      
      <MasterDetailPage.Master>
            <pages:MDPageMaster x:Name="MasterPage" />
      </MasterDetailPage.Master>
    
      <MasterDetailPage.Detail>
            <NavigationPage BarBackgroundColor="#ff8026" x:Name = "NavPage">
                <x:Arguments>
                    <pages:CoworkingsMapPage/>
                </x:Arguments>
            </NavigationPage>
      </MasterDetailPage.Detail>
</MasterDetailPage>